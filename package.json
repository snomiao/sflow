{
  "name": "snoflow",
  "version": "1.3.5",
  "author": "snomiao <snomiao@gmail.com>",
  "module": "index.ts",
  "devDependencies": {
    "@types/bun": "^1.1.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./index.ts"
    },
    "./fromNodeStream": {
      "import": "./dist/fromNodeStream.js",
      "types": "./fromNodeStream.ts"
    }
  },
  "description": "Type-safety piping paradigram through webstream-kernels.",
  "files": [
    "*.ts",
    "dist"
  ],
  "keywords": [
    "webstream",
    "functional",
    "pipe-through",
    "kernel",
    "transform",
    "stream",
    "bun",
    "node",
    "browser"
  ],
  "scripts": {
    "build": "bun build index.ts fromNodeStream.ts --outdir=dist",
    "prerelease": "bun run build && bun run test",
    "release": "bunx standard-version && git push --follow-tags && npm publish",
    "test": "bun test"
  },
  "type": "module",
  "types": "./index.ts",
  "dependencies": {
    "@snomiao/die": "^1.0.9",
    "rambda": "^9.2.1",
    "web-streams-extensions": "^0.12.0"
  }
}
